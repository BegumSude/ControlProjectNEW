
@{
    Layout = null;
}



<!DOCTYPE html>
<html lang="en">
<head>
    <!-- Required meta tags -->
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">
    <title>Admin Paneli</title>
    <!-- base:css -->
    <link rel="stylesheet" href="~/Templates/Admin/template/vendors/typicons.font/font/typicons.css">
    <link rel="stylesheet" href="~/Templates/Admin/template/vendors/css/vendor.bundle.base.css">
    <!-- endinject -->
    <!-- plugin css for this page -->
    <!-- End plugin css for this page -->
    <!-- inject:css -->
    <link rel="stylesheet" href="~/Templates/Admin/template/css/vertical-layout-light/style.css">
    <!-- endinject -->
    <link rel="shortcut icon" href="~/Templates/Admin/template/images/favicon.png" />

    <style>
        .nav .nav-item .nav-link.active {
            background-color: #4b49ac;
            color: #fff;
            font-weight: bold;
            border-radius: 5px;
        }

        .nav .nav-item .nav-link:hover {
            background-color: #5c58c9;
            color: #fff;
        }
    </style>

</head>

<body>


    <div class="container-scroller">

        <!-- Navbar-->
        @Html.Action("AdminLayoutNavbar", "AdminLayout")

        <!-- partial  Sidebar-Navbar Renk Ayar Paneli -->
        <div class="container-fluid page-body-wrapper">
            <!-- partial:partials/_settings-panel.html -->


            <div class="theme-setting-wrapper">
                <div id="settings-trigger"><i class="typcn typcn-cog-outline"></i></div>
                <div id="theme-settings" class="settings-panel">
                    <i class="settings-close typcn typcn-delete-outline"></i>
                    <p class="settings-heading">Sidebar Rengi</p>
                    <div class="sidebar-bg-options" id="sidebar-light-theme">
                        <div class="img-ss rounded-circle bg-light border mr-3"></div>
                        Açık
                    </div>
                    <div class="sidebar-bg-options selected" id="sidebar-dark-theme">
                        <div class="img-ss rounded-circle bg-dark border mr-3"></div>
                        Koyu
                    </div>
                    <p class="settings-heading mt-2">Header Rengi</p>
                    <div class="color-tiles mx-0 px-4">
                        <div class="tiles success"></div>
                        <div class="tiles warning"></div>
                        <div class="tiles danger"></div>
                        <div class="tiles primary"></div>
                        <div class="tiles info"></div>
                        <div class="tiles dark"></div>
                        <div class="tiles default border"></div>


                    </div>
                </div>
            </div>


            <nav class="sidebar sidebar-offcanvas" id="sidebar">
                <ul class="nav">
                    <li class="nav-item">
                        <div class="d-flex sidebar-profile">
                            <div class="sidebar-profile-image">
                                <img src="images/faces/face29.png" alt="image">
                                <span class="sidebar-status-indicator"></span>
                            </div>
                            <div class="sidebar-profile-name">
                                <p class="sidebar-name">
                                    AcunMedya Akademi
                                </p>
                                <p class="sidebar-designation">
                                    Hoş Geldiniz!
                                </p>
                            </div>
                        </div>
                        <div class="nav-search">
                            <div class="input-group">
                                <input type="text" class="form-control" placeholder="Type to search..." aria-label="search" aria-describedby="search">
                                <div class="input-group-append">
                                    <span class="input-group-text" id="search">
                                        <i class="typcn typcn-zoom"></i>
                                    </span>
                                </div>
                            </div>
                        </div>
                    </li>
                    <li class="nav-item">
                        <a class="nav-link" href="index.html">
                            <i class="typcn typcn-device-desktop menu-icon"></i>
                            <span class="menu-title">Dashboard <span class="badge badge-primary ml-3">New</span></span>
                        </a>
                    </li>
                    <li class="nav-item">
                        <p class="sidebar-menu-title">Eğitmenler</p>
                    </li>


                    <li class="nav-item">
                        <a class="nav-link" data-toggle="collapse" href="#murat" aria-expanded="false" aria-controls="murat">
                            <i class="typcn typcn-briefcase menu-icon"></i>
                            <span class="menu-title">Murat Yücedağ</span>
                        </a>
                        <div class="collapse" id="murat">
                            <ul class="nav flex-column sub-menu">
                                <li class="nav-item">
                                    <a class="nav-link" href="/MuratYucedag/Index">Ayrıntılar</a>
                                </li>
                            </ul>
                        </div>
                    </li>


                    <li class="nav-item">
                        <a class="nav-link" data-toggle="collapse" href="#erhan" aria-expanded="false" aria-controls="erhan">
                            <i class="typcn typcn-briefcase menu-icon"></i>
                            <span class="menu-title">Erhan Gündüz</span>
                        </a>
                        <div class="collapse" id="erhan">
                            <ul class="nav flex-column sub-menu">
                                <li class="nav-item">
                                    <a class="nav-link" href="/ErhanGunduz/Index">Ayrıntılar</a>
                                </li>
                            </ul>
                        </div>
                    </li>


                    <li class="nav-item">
                        <a class="nav-link" data-toggle="collapse" href="#busenur" aria-expanded="false" aria-controls="busenur">
                            <i class="typcn typcn-briefcase menu-icon"></i>
                            <span class="menu-title">Buse Nur Demirbaş</span>
                        </a>
                        <div class="collapse" id="busenur">
                            <ul class="nav flex-column sub-menu">
                                <li class="nav-item">
                                    <a class="nav-link" href="/BuseNurDemirbas/Index">Ayrıntılar</a>
                                </li>
                            </ul>
                        </div>
                    </li>

                    <li class="nav-item">
                        <a class="nav-link" data-toggle="collapse" href="#abdullah" aria-expanded="false" aria-controls="abdullah">
                            <i class="typcn typcn-briefcase menu-icon"></i>
                            <span class="menu-title">Abdullah Kuş</span>
                        </a>
                        <div class="collapse" id="abdullah">
                            <ul class="nav flex-column sub-menu">
                                <li class="nav-item">
                                    <a class="nav-link" href="/AbdullahKus/Index">Ayrıntılar</a>
                                </li>
                            </ul>
                        </div>
                    </li>

                    <li class="nav-item">
                        <a class="nav-link" data-toggle="collapse" href="#abdullah" aria-expanded="false" aria-controls="abdullah">
                            <i class="typcn typcn-briefcase menu-icon"></i>
                            <span class="menu-title">Münire Çelikbilek</span>
                        </a>
                        <div class="collapse" id="abdullah">
                            <ul class="nav flex-column sub-menu">
                                <li class="nav-item">
                                    <a class="nav-link" href="/MunireCelikbilek/Index">Ayrıntılar</a>
                                </li>
                            </ul>
                        </div>
                    </li>

                    <li class="nav-item">
                        <a class="nav-link" data-toggle="collapse" href="#abdullah" aria-expanded="false" aria-controls="abdullah">
                            <i class="typcn typcn-briefcase menu-icon"></i>
                            <span class="menu-title">Fatih Akbulut</span>
                        </a>
                        <div class="collapse" id="abdullah">
                            <ul class="nav flex-column sub-menu">
                                <li class="nav-item">
                                    <a class="nav-link" href="/FatihAkbulut/Index">Ayrıntılar</a>
                                </li>
                            </ul>
                        </div>
                    </li>
                </ul>
            </nav>

            <!-- Ana Ekran-->
            <div class="main-panel">
                <div class="content-wrapper">

                    @RenderBody()
                </div>

                <!-- partial -->
            </div>
            <!-- Ana Ekran Sonu -->
        </div>
        <!-- page-body-wrapper ends -->
    </div>



    <!-- container-scroller -->
    <!-- base:js -->
    <script src="~/Templates/Admin/template/vendors/js/vendor.bundle.base.js"></script>
    <!-- endinject -->
    <!-- Plugin js for this page-->
    <!-- End plugin js for this page-->
    <!-- inject:js -->
    <script src="~/Templates/Admin/template/js/off-canvas.js"></script>
    <script src="~/Templates/Admin/template/js/hoverable-collapse.js"></script>
    <script src="~/Templates/Admin/template/js/template.js"></script>
    <script src="~/Templates/Admin/template/js/settings.js"></script>
    <script src="~/Templates/Admin/template/js/todolist.js"></script>
    <!-- endinject -->
    <!-- plugin js for this page -->
    <script src="~/Templates/Admin/template/vendors/progressbar.js/progressbar.min.js"></script>
    <script src="~/Templates/Admin/template/vendors/chart.js/Chart.min.js"></script>
    <!-- End plugin js for this page -->
    <!-- Custom js for this page-->
    <script src="~/Templates/Admin/template/js/dashboard.js"></script>
    <script src="../../vendors/js/vendor.bundle.base.js"></script>
    <!-- End plugin js for this page -->
    <!-- Custom js for this page-->
    <script src="~/Templates/Admin/template/js/chart.js"></script>
    <script src="https://cdn.jsdelivr.net/npm/progressbar.js"></script>
    <script src="~/Templates/Admin/template/vendors/progressbar.js/progressbar.min.js"></script>
    <!-- End custom js for this page-->
    <!-- End custom js for this page-->
</body>
</html>